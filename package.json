{
  "name": "font-converter-tool",
  "version": "1.0.0",
  "description": "A desktop application for converting TTF fonts to WOFF2 format with multi-language subset support",
  "main": "src/font_trans.py",
  "scripts": {
    "start": "python src/font_trans.py",
    "build": "pyinstaller FontTool.spec",
    "split-zh": "python src/font_splitter.py assets/fonts/YourFont.ttf --language zh",
    "split-tc": "python src/font_splitter.py assets/fonts/YourFont.ttf --language tc",
    "split-ja": "python src/font_splitter.py assets/fonts/YourFont.ttf --language ja"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenyx0517/work-utils.git"
  },
  "keywords": [
    "font",
    "converter",
    "woff2",
    "ttf",
    "desktop",
    "app"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chenyx0517/work-utils/issues"
  },
  "homepage": "https://github.com/chenyx0517/work-utils#readme",
  "dependencies": {
    "@diezhi/momo": "^6.1.10",
    "@diezhi/pal": "^7.4.8",
    "@types/ali-oss": "^6.16.11",
    "ali-oss": "^6.23.0"
  }
}